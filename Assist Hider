game:GetService("StarterGui"):SetCore("SendNotification", { -- just a lil message to lyk that it was executed and hidden
	Title = "Appeal Assist Hider",
	Text = "Assist was Hidden! Have fun!",
	Duration = .1,
})

while task.wait(.5) do
	
	task.wait(3)
	
	if game:GetService("CoreGui"):FindFirstChild("Aim Assistant [2.0.0]") then
		game:GetService("CoreGui")["Aim Assistant [2.0.0]"].MainFrame.Visible = false
		game:GetService("CoreGui")["Aim Assistant [2.0.0]"].Circle.Visible = false
		
		task.wait()
		
		if game:GetService("CoreGui")["Aim Assistant [2.0.0]"]:FindFirstChild("Highlight") then
			game:GetService("CoreGui")["Aim Assistant [2.0.0]"]:FindFirstChild("Highlight"):Destroy()
		end
		
		
	end
end
