local RunService = game:GetService("RunService")

RunService.Heartbeat:Connect(function()
   pcall(function()
       for i,v in pairs(game.Players:GetChildren()) do
           if v.Name ~= game.Players.LocalPlayer.Name then
               local hrp = v.Character.HumanoidRootPart
	       local Head = v.Charcater.Head
	       local Upper = v.Character.UpperTorso
               hrp.Velocity = Vector3.new(hrp.Velocity.X, 0, hrp.Velocity.Z)    
               hrp.AssemblyLinearVelocity = Vector3.new(hrp.Velocity.X, 0, hrp.Velocity.Z, hrp.Velocity.Y, 0)
	       Head.Velocity = Vector3.new(hrp.Velocity.X, 0, hrp.Velocity.Z)    
               Head.AssemblyLinearVelocity = Vector3.new(hrp.Velocity.X, 0, hrp.Velocity.Z, hrp.Velocity.Y, 0)
	       Upper.Velocity = Vector3.new(hrp.Velocity.X, 0, hrp.Velocity.Z)    
               Upper.AssemblyLinearVelocity = Vector3.new(hrp.Velocity.X, 0, hrp.Velocity.Z, hrp.Velocity.Y, 0)
           end
       end
   end)
end)
