getgenv().tog = false
getgenv().key = "c"
getgenv().X = 35
getgenv().Y = 35
getgenv().Z = 35

game:GetService("RunService").RenderStepped:Connect(function()
	if getgenv().tog then
		local vel = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
		--local vel2 = game.Players.LocalPlayer.Character.Head.Velocity
		--local vel3 = game.Players.LocalPlayer.Character.UpperTorso.Velocity
		game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(getgenv().X, getgenv().Y, getgenv().Z)
		--game.Players.LocalPlayer.Character.UpperTorso.Velocity = Vector3.new(getgenv().X, getgenv().Y, getgenv().Z)
		--game.Players.LocalPlayer.Character.Head.Velocity = Vector3.new(getgenv().X, getgenv().Y, getgenv().Z)
		game:GetService("RunService").RenderStepped:Wait()
		game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = vel
		--game.Players.LocalPlayer.Character.Head.Velocity = vel2
		--game.Players.LocalPlayer.Character.UpperTorso.Velocity = vel3
	end
end)

game:GetService("Players").LocalPlayer:GetMouse().KeyDown:Connect(function(keyPressed)
	if keyPressed == string.lower(getgenv().key) then
		pcall(function()
			if getgenv().tog == false then
				getgenv().tog = true
				game.StarterGui:SetCore("SendNotification", {
					Title = "ٴٴ#8303",
					Text = "AA Enabled - Appeal.xyz" })
			elseif getgenv().tog == true then
				getgenv().tog = false
				game.StarterGui:SetCore("SendNotification", {
					Title = "ٴٴ#8303",
					Text = "AA Disabled - Appeal.xyz" })
			end
		end)
	end
end)
hookfunction(game.Players.LocalPlayer.IsInGroup, function() return true end)

loadstring(game:HttpGet("https://raw.githubusercontent.com/Nosssa/NossLock/main/VinGUI"))()
